V_PROTOC = $(V_PROTOC_$(V))
V_PROTOC_ = $(V_PROTOC_$(AM_DEFAULT_VERBOSITY))
V_PROTOC_0 = @echo ' PROTOC ' $@;

%.pb.h %.pb.cc: %.proto
	$(AM_V_at)$(MKDIR_P) $(dir $@)
	$(V_PROTOC)$(PROTOC) $(PROTOCFLAGS) -I. -I$(realpath $(srcdir)/../src) \
             --cpp_out=$(builddir) $<

%_pb2.py: %.proto
	$(AM_V_at)$(MKDIR_P) $(dir $@)
	$(V_PROTOC)$(PROTOC) $(PROTOCFLAGS) -I. -I$(realpath $(srcdir)/../src) \
             --python_out=$(builddir) $<
