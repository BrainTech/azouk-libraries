#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_configure:
	./bootstrap.sh

override_dh_auto_build:

override_dh_install:
	dh_install
	find debian/multiplexer-source -name .gitignore -delete , \
                                       -name '*~' -delete
